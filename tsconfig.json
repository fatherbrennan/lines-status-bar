{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2020", "WebWorker"],
    "module": "CommonJS",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
    "target": "ES2015"
  },
  "include": ["src"]
}
